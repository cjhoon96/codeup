    "import sys\n",
    "sys.setrecursionlimit(1000000)  ## 재귀 최대범위를 변경\n",
    "sum_dic={1:1}\n",
    "def n_sum(a):\n",
    "    if a in sum_dic:\n",
    "        return sum_dic[a]\n",
    "    else:\n",
    "        sum_dic[a]=n_sum(a-1)+a\n",
    "        return sum_dic[a]\n",
    "print(n_sum(int(input())))"
   ]
